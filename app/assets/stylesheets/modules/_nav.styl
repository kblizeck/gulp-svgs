// A module should be something that can stand by itself regardless of
//  its container. You may have modules like _social-icons.styl,
//  _navigation.styl or _cta.styl
// ===================================================================
nav {
  ul {
    margin: 0;
    padding: 0;
    list-style: none;
    li {
      float: left;
      padding: 1rem;
      svg {
        width: 50px;
        height: 50px;
      }
      &:nth-child(odd) {
        svg {
          fill: purple;
        }
      }
      &:nth-child(even){
        svg {
          fill: green;
        }
      }
    }
  }
}
